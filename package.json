{
  "name": "test",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:old": "run-p type-check build-only",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@unocss/reset": "^0.53.4",
    "axios": "^1.4.0",
    "countup.js": "^2.7.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.7",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.0",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@unocss/postcss": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "5.0.0-alpha.2",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.6",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.3"
  },
  "engines": {
    "node": "^16 || >=16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
